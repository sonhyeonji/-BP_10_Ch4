x=input("색상 #1을 입력하시오.:")
y=input("색상 #2을 입력하시오.:")
z=input("색상 #3을 입력하시오.:")

import turtle
t=turtle.Turtle()
t.shape("turtle")
t.fillcolor(x)
t.begin_fill()
t.circle(50)
t.end_fill()
t.penup()
t.setx(100)
t.pendown()
t.fillcolor(y)
t.begin_fill()
t.circle(50)
t.end_fill()
t.penup()
t.setx(200)
t.pendown()
t.fillcolor(z)
t.begin_fill()
t.circle(50)
t.end_fill()
t._screen.exitonclick()
