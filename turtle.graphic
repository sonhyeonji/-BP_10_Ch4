x1=int(input("x1의 좌표를 입력하시오.:"))
y1=int(input("y1의 좌표를 입력하시오.:"))
x2=int(input("x2의 좌표를 입력하시오.:"))
y2=int(input("y2의 좌표를 입력하시오.:"))
x3=int(input("x3의 좌표를 입력하시오.:"))
y3=int(input("y3의 좌표를 입력하시오.:"))

list=[x1,y1,x2,y2,x3,y3]
import turtle
t=turtle.Turtle()
t.shape("turtle")
t.up()
t.goto(list[0],list[1])
t.down()
t.goto(list[2],list[3])
t.goto(list[4],list[5])
