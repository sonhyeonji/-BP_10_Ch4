x=input("색상 #1을 입력하시오.:")
y=input("색상 #2을 입력하시오.:")
z=input("색상 #3을 입력하시오.:")
colors_list=[x,y,z]

import turtle
t=turtle.Turtle()
t.shape("turtle")
t.fillcolor(colors_list[0])
t.begin_fill()
t.circle(50)
t.end_fill()
t.penup()
t.setx(100)
t.pendown()
t.fillcolor(colors_list[1])
t.begin_fill()
t.circle(50)
t.end_fill()
t.penup()
t.setx(200)
t.pendown()
t.fillcolor(colors_list[2])
t.begin_fill()
t.circle(50)
t.end_fill()
t._screen.exitonclick()

